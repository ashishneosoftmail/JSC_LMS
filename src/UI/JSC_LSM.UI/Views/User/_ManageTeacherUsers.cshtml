@{
    ViewData["Title"] = "ManageTeacherUsers";
}



<div class="row">
    <div class="col-sm-3">
        <div class="form-group row">
            <label for="schoolName" class="col-sm-5 col-form-label">School Name</label>
            <div class="col-sm-7">
                <select class="form-control" id="schoolName">
                    <option disabled selected hidden>Select School</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="form-group row">
            <label for="className" class="col-sm-5 col-form-label">Class Name</label>
            <div class="col-sm-7">
                <select class="form-control" id="className">
                    <option disabled selected hidden>Select Class</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="form-group row">
            <label for="section" class="col-sm-5 col-form-label">Section</label>
            <div class="col-sm-7">
                <select class="form-control" id="section">
                    <option disabled selected hidden>Select Section</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="form-group row">
            <label for="subject" class="col-sm-5 col-form-label">Subject</label>
            <div class="col-sm-7">
                <select class="form-control" id="subject">
                    <option disabled selected hidden>Select Subject</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
        </div>
    </div>

</div>

<div class="row mt-3">
    <div class="col-sm-3">
        <div class="form-group row">
            <label for="status" class="col-sm-5 col-form-label">Status</label>
            <div class="col-sm-7">
                <select class="form-control" id="status">
                    <option disabled selected hidden>Select Status</option>
                    <option>Active</option>
                    <option>Inactive</option>
                </select>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="form-group row">
            <label for="date" class="col-sm-5 col-form-label">Date</label>
            <div class="col-sm-7">
                <input type="date" value="" class="form-control" id="date" />
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="form-group row">
            <label for="teacher" class="col-sm-5 col-form-label">Teacher Name</label>
            <div class="col-sm-7">
                <select class="form-control" id="teacher">
                    <option disabled selected hidden>Select Teacher</option>
                    <option>1</option>
                    <option>2</option>
                </select>
            </div>
        </div>
    </div>
    <div class="col-sm-1">
        <div class="form-group row">
            <button class="btn btn-primary">Search</button>
        </div>
    </div>
</div>
<div class="row mt-3">
    <div class="col-sm-12">
        <button class="btn btn-sm btn-primary pull-right m-2">Export</button>
