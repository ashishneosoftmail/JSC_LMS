
@{
    ViewData["Title"] = "Profile";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}

<style>
    #div6 {
        position: relative;
        display: inline-block;
        border-style: solid;
        border-color: black;
    }
</style>
<div id="wrapper">
    @if (ViewBag.UpdateSuperadminSuccess != null)
    {
        <script>alert("@ViewBag.UpdateSuperadminSuccess")</script>
    }
    @if (ViewBag.UpdateSuperadminError != null)
    {
        <script>alert("@ViewBag.UpdateSuperadminError")</script>
    }
</div>
<h2 class="mt-4">Manage Profile</h2>

<div class="container" id="div6">
    @{ Html.RenderPartial("_Profile"); }
    @{ Html.RenderPartial("_ChangePassword"); }
    @{ Html.RenderPartial("_ImageLogo"); }
</div>
